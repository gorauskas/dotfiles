{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#7eb8da",
                    "style": "plain",
                    "template": "\u250f[<#ffffff>\uf508</> {{ .UserName }} from <#ffffff>\ufcbe</> {{ .HostName }}]",
                    "type": "session"
                },
                {
                    "foreground": "#7eb8da",
                    "properties": {
                        "style": "austin",
                        "threshold": 0
                    },
                    "style": "diamond",
                    "template": "[<#ffffff>\uf252</> <#ffd800>{{ .FormattedMs }}</>]",
                    "type": "executiontime"
                },
                {
                    "foreground": "#7eb8da",
                    "properties": {
                        "time_format": "2006-01-02 15:04:05"
                    },
                    "style": "diamond",
                    "template": "[<#ffffff>\uf5ef</> {{ .CurrentDate | date .Format }}]",
                    "type": "time"
                },
                {
                    "foreground": "#7eb8da",
                    "properties": {
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "plain",
                    "template": "[<#ffffff>{{ .UpstreamIcon }}</> <#67b11d>{{ .HEAD }}</>{{if .BranchStatus }}<#67b11d>{{ .BranchStatus }}</>{{ end }}{{ if .Working.Changed }} <#ffffff>\uf044</> <#67b11d>{{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} <#67b11d>|</>{{ end }}{{ if .Staging.Changed }} <#ffffff>\uf046</> <#67b11d>{{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <#ffffff>\uf692</> <#67b11d>{{ .StashCount }}</>{{ end }}]",
                    "type": "git"
                },
                {
                    "foreground": "#7eb8da",
                    "style": "plain",
                    "template": "[<#ffffff>\ufc8d</> <#ee3351>ERROR: {{ .Code }}</>{{ if .Meaning }} <#ffffff>=></> <#ee3351>{{ .Meaning }}</>{{ end }}]",
                    "type": "exit"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#7eb8da",
                    "style": "plain",
                    "properties": {
                        "style": "full"
                    },
                    "template": "\u2516[<#ffffff>\uE235</> <#ffd800>{{ .Venv }}</><#ffffff>::</><#ffd800>{{ .Full }}</>]",
                    "type": "python"
                },
                {
                    "foreground": "#7eb8da",
                    "properties": {
                        "style": "full"
                    },
                    "style": "plain",
                    "template": "{{ if not .Env.VIRTUAL_ENV }}\u2516{{ end }}[<#67b11d>{{ .Path }}</>]",
                    "type": "path"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#7eb8da",
                    "style": "plain",
                    "template": "\u2514\u2500\u27a4",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}