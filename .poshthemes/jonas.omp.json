{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#7eb8da",
                    "style": "plain",
                    "template": "\u250f[{{ .UserName }}@{{ .HostName }}]",
                    "type": "session"
                },
                {
                    "foreground": "#ff7900",
                    "properties": {
                        "style": "austin",
                        "threshold": 0
                    },
                    "style": "diamond",
                    "template": "<#7eb8da>[</>{{ .FormattedMs }}<#7eb8da>]</>",
                    "type": "executiontime"
                },
                {
                    "foreground": "#7eb8da",
                    "properties": {
                        "time_format": "2006-01-02 15:04:05"
                    },
                    "style": "diamond",
                    "template": "[{{ .CurrentDate | date .Format }}]",
                    "type": "time"
                },
                {
                    "foreground": "#67b11d",
                    "properties": {
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "plain",
                    "template": "<#7eb8da>[</>{{ .HEAD }}{{if .BranchStatus }}{{ .BranchStatus }}{{ end }}{{ if or .Working.Changed .Staging.Changed }}\u200A<#999999>\u276A</><#6666CC>\u2213{{ end }}{{ if .Working.Changed }}\u200A{{ .Working.String }}\u2297{{ end }}{{ if .Staging.Changed }}\u200A{{ .Staging.String }}\u2295{{ end }}{{ if gt .StashCount 0 }}\u200A{{ .StashCount }}\u229A{{ end }}{{ if or .Working.Changed .Staging.Changed }}</><#999999>\u276B</>{{ end }}<#7eb8da>]</>",
                    "type": "git"
                },
                {
                    "foreground": "#CE2029",
                    "style": "plain",
                    "template": "<#7eb8da>[</>ERR:{{ .Code }}{{ if .Meaning }}<#999999>=></>{{ .Meaning }}{{ end }}<#7eb8da>]</>",
                    "type": "exit"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#7eb8da",
                    "template": "\u2517"
                },
                {
                    "foreground": "#ffd800",
                    "style": "plain",
                    "template": "<#7eb8da>[</>Py:{{ .Full }}{{ if .Venv }}<#999999>::</>{{ .Venv }}{{ end }}<#7eb8da>]</>",
                    "type": "python"
                },
                {
                    "type": "java",
                    "style": "plain",
                    "foreground": "#ffd800",
                    "template": "<#7eb8da>[</><b>JVM</b>:{{ .Major }}<#7eb8da>]</>",
                    "properties": {
                        "fetch_version": true,
                        "display_mode": "files"
                    }
                },
                {
                    "type": "kotlin",
                    "style": "plain",
                    "foreground": "#ffd800",
                    "template": "<#7eb8da>[</><b>K</b>:{{ .Full }}<#7eb8da>]</>",
                    "properties": {
                        "fetch_version": true,
                        "display_mode": "files"
                    }
                },
                {
                    "foreground": "#7eb8da",
                    "properties": {
                        "style": "full"
                    },
                    "style": "plain",
                    "template": "[<#67b11d>{{ .Path }}</>]",
                    "type": "path"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#7eb8da",
                    "style": "plain",
                    "template": "<#999999>\u03bb</> \u27a4",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}