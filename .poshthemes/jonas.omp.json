{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#81A1C1",
                    "style": "plain",
                    "template": "\u250f[{{ .UserName }}@{{ .HostName }}]",
                    "type": "session"
                },
                {
                    "foreground": "#D08770",
                    "properties": {
                        "style": "austin",
                        "threshold": 0
                    },
                    "style": "diamond",
                    "template": "<#81A1C1>[</>{{ .FormattedMs }}<#81A1C1>]</>",
                    "type": "executiontime"
                },
                {
                    "foreground": "#81A1C1",
                    "properties": {
                        "time_format": "2006-01-02 15:04:05"
                    },
                    "style": "diamond",
                    "template": "[{{ .CurrentDate | date .Format }}]",
                    "type": "time"
                },
                {
                    "foreground": "#A3BE8C",
                    "properties": {
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "plain",
                    "template": "<#81A1C1>[</>{{ .HEAD }}{{if .BranchStatus }}{{ .BranchStatus }}{{ end }}{{ if or .Working.Changed .Staging.Changed }}\u200A<#4C566A>\u276A</><#B48EAD>\u2213{{ end }}{{ if .Working.Changed }}\u200A{{ .Working.String }}\u2297{{ end }}{{ if .Staging.Changed }}\u200A{{ .Staging.String }}\u2295{{ end }}{{ if gt .StashCount 0 }}\u200A{{ .StashCount }}\u229A{{ end }}{{ if or .Working.Changed .Staging.Changed }}</><#4C566A>\u276B</>{{ end }}<#81A1C1>]</>",
                    "type": "git"
                },
                {
                    "foreground": "#BF616A",
                    "style": "plain",
                    "template": "<#81A1C1>[</>ERR:{{ .Code }}{{ if .Meaning }}<#4C566A>=></>{{ .Meaning }}{{ end }}<#81A1C1>]</>",
                    "type": "exit"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#81A1C1",
                    "template": "\u2517"
                },
                {
                    "foreground": "#EBCB8B",
                    "style": "plain",
                    "template": "<#81A1C1>[</>Py:{{ .Full }}{{ if .Venv }}<#4C566A>::</>{{ .Venv }}{{ end }}<#81A1C1>]</>",
                    "type": "python"
                },
                {
                    "type": "java",
                    "style": "plain",
                    "foreground": "#EBCB8B",
                    "template": "<#81A1C1>[</><b>JVM</b>:{{ .Major }}<#81A1C1>]</>",
                    "properties": {
                        "fetch_version": true,
                        "display_mode": "files"
                    }
                },
                {
                    "type": "kotlin",
                    "style": "plain",
                    "foreground": "#EBCB8B",
                    "template": "<#81A1C1>[</><b>K</b>:{{ .Full }}<#81A1C1>]</>",
                    "properties": {
                        "fetch_version": true,
                        "display_mode": "files"
                    }
                },
                {
                    "foreground": "#81A1C1",
                    "properties": {
                        "style": "full"
                    },
                    "style": "plain",
                    "template": "[<#A3BE8C>{{ .Path }}</>]",
                    "type": "path"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#81A1C1",
                    "style": "plain",
                    "template": "<#D8DEE9>\u03bb</> \u27a4",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}