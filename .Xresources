!! URxvt definitions
URxvt*termName: rxvt
URxvt*transparent: false
URxvt*inheritPixmap: false
URxvt*imLocale: en_US.UTF-8
URxvt*scrollBar: true
URxvt*scrollBar_right:false
URxvt*scrollstyle:rxvt
URxvt*saveLines: 4096
URxvt*cursorBlink: true
URxvt*geometry: 160x50
URxvt*letterSpace: -1

URxvt.foreground: #dcdccc
URxvt*background: #3f3f3f

URxvt.saveLines:32767
URxvt.scrollBar: false

URxvt*font: xft:DejaVu Sans Mono:pixelsize=11
URxvt*boldFont: xft:DejaVu Sans Mono:bold:pixelsize=11

URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: google-chrome
URxvt.matcher.button: 2
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

URxvt*colorUL: #c5f779
URxvt*underlineColor: #c5f779

URxvt*color0: #3f3f3f
URxvt*color1: #705050
URxvt*color2: #60b48a
URxvt*color3: #dfaf8f
URxvt*color4: #506070
URxvt*color5: #dc8cc3
URxvt*color6: #8cd0d3
URxvt*color7: #DCDCCC

URxvt*color8:  #709080
URxvt*color9:  #cc9393
URxvt*color10: #7f9f7f
URxvt*color11: #f0dfaf
URxvt*color12: #94bff3
URxvt*color13: #ec93d3
URxvt*color14: #93e0e3
URxvt*color15: #ffffff

!! XTerm definitions
XTerm*vt100*geometry: 160x60
XTerm*background: #3f3f3f
XTerm*foreground: #dcdccc
XTerm*pointerColor: firebrick
XTerm*pointerColorBackground: black
XTerm*cursorColor: navy
XTerm*internalBorder: 3
XTerm*loginShell: true
XTerm*scrollBar: false
XTerm*scrollKey: true
XTerm*saveLines: 4096
XTerm*multiClickTime: 250
XTerm*faceName: DejaVu Sans Mono
XTerm*faceSize: 9

